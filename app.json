{
  "expo": {
    "name": "College-App",
    "slug": "College-Results-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "host.vrsec.collegeapp",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "com.vrsec.college_app",
      "versionCode": 1,
      "permissions": ["USE_FINGERPRINT", "USE_BIOMETRIC"]
    }
  }
}
